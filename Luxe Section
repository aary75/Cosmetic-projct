    document.getElementById("demo-head1").style.borderBottomLeftRadius = "0";
    document.getElementById("demo2").style.position = "relative";
    display1 = 0;
  }
  else{
    document.getElementById("demo2").style.visibility = "hidden";
    document.getElementById("demo-head1").style.borderBottomRightRadius = "5px";
    document.getElementById("demo-head1").style.borderBottomLeftRadius = "5px";
    document.getElementById("demo2").style.position = "absolute";
