    var display  = 0;
function myFunction() {
  if(display == 1){
    document.getElementById("demo1").style.visibility = "visible";
    document.getElementById("demo").style.borderBottomRightRadius = "0";
    document.getElementById("demo").style.borderBottomLeftRadius = "0";
    document.getElementById("parts").style.marginTop = "310px";
    display = 0;
  }
  else{
    document.getElementById("demo1").style.visibility = "hidden";
    document.getElementById("demo").style.borderBottomRightRadius = "5px";
    document.getElementById("demo").style.borderBottomLeftRadius = "5px";
    document.getElementById("parts").style.marginTop = "10px";
    display = 1;
  }
}

